<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#FDD550</item>
        <item name="colorPrimaryDark">#FFC619</item>
        <item name="android:textColorPrimary">@color/black</item>
        <item name="colorAccent">#000000</item>
        <item name="android:textColor">@color/black</item>
    </style>


    <style name="CanaryTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">#FFEB3B</item>
        <item name="colorPrimaryDark">#FFC619</item>
        <item name="colorAccent">#F4EB97</item>
    </style>
    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.selectorColor" parent="Widget.MaterialComponents.NavigationView" >
        <item name="itemShapeFillColor">@color/black</item>
        <item name="itemIconTint">@color/black</item>
        <item name="itemTextColor">@color/black</item>
    </style>



    <style name="AppTheme.PopupOverlay" parent="Theme.AppCompat.Light">
        <item name="colorPrimary">#FDD550</item>
        <item name="colorPrimaryDark">#FFC619</item>
        <item name="android:textColorPrimary">@color/black</item>
        <item name="colorAccent">#000000</item>
        <item name="android:textColor">@color/black</item>
    </style>


    <style name="mytoast">
        <item name="stTextBold">true</item>
        <item name="stTextColor">#fff</item>
        <item name="stTextSize">14sp</item>
        <item name="stColorBackground">#8BC34A</item>
        <item name="stSolidBackground">true</item>
        <item name="stIconStart">@drawable/bird</item>
        <item name="stLength">LONG</item>
        <item name="stRadius">20dp</item>

    </style>
    <style name="mytoast2">
        <item name="stTextBold">true</item>
        <item name="stTextColor">#f4fa4d</item>
        <item name="stTextSize">14sp</item>
        <item name="stColorBackground">#27d986</item>
        <item name="stSolidBackground">true</item>
        <item name="stIconStart">@drawable/bird</item>
        <item name="stLength">LONG</item>
        <item name="stRadius">20dp</item>
    </style>


</resources>